{
  "name": "@maas/vue-equipment",
  "description": "Our Frontend Toolkit, Free and Open Source",
  "version": "0.40.0",
  "contributors": [
    {
      "name": "Robin Scholz",
      "url": "https://github.com/robinscholz"
    },
    {
      "name": "Christoph Jeworutzki",
      "url": "https://github.com/ChristophJeworutzki"
    }
  ],
  "devDependencies": {
    "@maas/config": "^1.5.0",
    "@release-it/bumper": "^7.0.1",
    "@types/node": "catalog:",
    "release-it": "^18.1.2",
    "tailwindcss": "catalog:",
    "turbo": "^2.4.1",
    "typescript": "catalog:"
  },
  "exports": {
    "./nuxt": {
      "types": "./dist/nuxt/types.d.mts",
      "import": "./dist/nuxt/module.mjs"
    },
    "./composables": {
      "types": "./dist/composables/index.d.mts",
      "import": "./dist/composables/index.mjs"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.mjs"
    },
    "./plugins/*/css/*.css": {
      "import": "./dist/plugins/*/src/css/*.css"
    },
    "./utils": {
      "types": "./dist/utils/index.d.mts",
      "import": "./dist/utils/index.mjs"
    },
    "./utils/css/*.css": {
      "import": "./dist/utils/css/*.css"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@floating-ui/vue": "^1",
    "@maas/magic-timer": "^1",
    "@maas/vue-autosize": "^1",
    "@maas/vue-primitive": "^1",
    "@nuxt/kit": "^3",
    "@vueuse/core": "^12",
    "@vueuse/integrations": "^12",
    "defu": "^6",
    "focus-trap": "^7",
    "hls.js": "^1",
    "luxon": "^3",
    "mitt": "^3",
    "motion": "^12",
    "nuxt": "^3.15",
    "universal-cookie": "^7",
    "vue": "^3.5",
    "wheel-gestures": "^2.2.48"
  },
  "peerDependenciesMeta": {
    "@floating-ui/vue": {
      "optional": false
    },
    "@nuxt/kit": {
      "optional": false
    },
    "@maas/magic-timer": {
      "optional": false
    },
    "@maas/vue-autosize": {
      "optional": false
    },
    "@maas/vue-primitive": {
      "optional": false
    },
    "@vueuse/core": {
      "optional": false
    },
    "@vueuse/integrations": {
      "optional": false
    },
    "defu": {
      "optional": false
    },
    "hls.js": {
      "optional": false
    },
    "luxon": {
      "optional": false
    },
    "mitt": {
      "optional": false
    },
    "motion": {
      "optional": false
    },
    "nuxt": {
      "optional": false
    },
    "universal-cookie": {
      "optional": false
    },
    "vue": {
      "optional": false
    },
    "wheel-gestures": {
      "optional": false
    }
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:docs": "turbo run dev --filter=docs",
    "dev:nuxt": "turbo run dev --filter=nuxt",
    "build": "turbo run build --filter=!playground",
    "build:types": "turbo run build:types",
    "build:docs": "turbo run build --filter=docs",
    "preview:docs": "turbo run preview --filter=docs",
    "update:metadata": "turbo run update --filter=metadata",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "turbo run lint",
    "uninstall": "pnpm dlx rimraf --glob ./**/node_modules --glob ./**/pnpm-lock.yaml",
    "release": "turbo run t:release && release-it",
    "update:deps": "pnpm dlx taze -r -w",
    "update:deps:major": "pnpm dlx taze major -r -w"
  },
  "type": "module",
  "volta": {
    "node": "20.18.1"
  },
  "packageManager": "pnpm@10.3.0",
  "pnpm": {
    "overrides": {
      "typescript": "catalog:"
    }
  }
}
